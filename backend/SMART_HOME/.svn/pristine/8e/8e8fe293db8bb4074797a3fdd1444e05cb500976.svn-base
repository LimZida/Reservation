<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>	
<!-- 일시정지 서비스(한시적)  -->
	<entry key="isService">false</entry>
	<entry key="dateFormat">yyyyMMdd HHmm</entry>
	<entry key="startDate">20170427 2100</entry>
	<entry key="endDate">20170427 2200</entry>
	<entry key="comment"></entry>
	<entry key="pauseTrList">CES0232</entry>

<!-- 일시정지 서비스(반복)  -->
	<entry key="isRepeat">false</entry>
	<entry key="repeatWeekdays">Sun, Mon, Tue, Wed, Thu, Fri, Sat</entry>
	<entry key="repeatTimeFormat">HHmm</entry>
	<entry key="repeatStartTime">0000</entry>
	<entry key="repeatEndTime">0100</entry>
	<entry key="repeatComment"></entry>
	<entry key="pauseRepeatTrList">ALL</entry>
	
	<!-- 어댑터 사용중지 서비스 -->
	<entry key="isStopTrService">false</entry>
	<entry key="stopTrList"></entry>
	<entry key="stopTrListComment"></entry>
</properties>