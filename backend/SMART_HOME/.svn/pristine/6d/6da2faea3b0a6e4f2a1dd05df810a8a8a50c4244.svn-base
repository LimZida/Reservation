<mapper namespace="RES1330S1">
	<delete id="DELETE_RESERVATION" statementType="PREPARED" parameterType="DefaultMap">
		<![CDATA[
			DELETE MCNC_TEST."RESERVE"
			WHERE
				"userSeqFK" = #{userSeqPK}
				AND "reserveSeqPK" = #{reserveSeqPK}
		]]>

	</delete>
</mapper>